# Visual Studio 2015 user specific files
FirstPerson/.vs/
Vr/.vs/

# Compiled Object files
*.slo
*.lo
*.o
*.obj

# Precompiled Headers
*.gch
*.pch

# Compiled Dynamic libraries
*.so
*.dylib
*.dll

# Fortran module files
*.mod

# Compiled Static libraries
*.lai
*.la
*.a
*.lib

# Executables
*.exe
*.out
*.app
*.ipa

# These project files can be generated by the engine
*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
FirstPerson/SourceArt/**/*.png
Vr/SourceArt/**/*.png
FirstPerson/SourceArt/**/*.tga
Vr/SourceArt/**/*.tga

# Binary Files
FirstPerson/Binaries/*
Vr/Binaries/*
FirstPerson/Plugins/**/Binaries/*
Vr/Plugins/**/Binaries/*

# Builds
FirstPerson/Build/*
Vr/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!FirstPerson/Build/*/
FirstPerson/Build/*/**
!FirstPerson/Build/*/PakBlacklist*.txt
!Vr/Build/*/
Vr/Build/*/**
!Vr/Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!FirstPerson/Build/**/*.ico
!Vr/Build/**/*.ico

# Built data for maps
FirstPerson/*_BuiltData.uasset
Vr/*_BuiltData.uasset

# Configuration files generated by the Editor
FirstPerson/Saved/*
Vr/Saved/*

# Compiled source files for the engine to use
FirstPerson/Intermediate/*
Vr/Intermediate/*
FirstPerson/Plugins/**/Intermediate/*
Vr/Plugins/**/Intermediate/*

# Cache files for the editor to use
FirstPerson/DerivedDataCache/*
Vr/DerivedDataCache/*
